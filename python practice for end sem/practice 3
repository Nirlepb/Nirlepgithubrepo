data= {"item1": 45.50, "item2":35, "item3": 41.30, "item4":55, "item5": 24}
max1=0
mid=0
small=0
for i in data:
    
    if data[i]>max1:
        small = mid
        mid = max1
        max1=data[i]
    elif max1>data[i]>mid:
        small = mid
        mid=data[i]
    elif  mid>data[i]>small:
        small=data[i]     
print("the max one ",max1)
print("the middle",mid)
print("the third one",small)        
          
        
        